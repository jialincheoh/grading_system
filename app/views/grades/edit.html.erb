<h2>Editing Grade</h2>

<%= render 'form' %>

<%= link_to 'Show', [@project, @grade], class: 'btn btn-small btn-warning' %>
<%= link_to 'Back to Listing Grades', project_grades_path(@project), class: 'btn btn-small btn-warning' %>
<%= link_to 'Back to Listing Projects', projects_path, class: 'btn btn-small btn-warning' %>
<%= link_to 'Back to Project', @project, class: 'btn btn-small btn-warning' %>

<% if @project.web_url.present? && current_grader.technical_expert? && (@project.graders.include? current_grader) %>
    <%= link_to 'Create Novelty Fields for User Experience Grader', project_novelty_fields_path(@project), class: 'btn btn-small btn-warning' %>
<% end %>

<%= link_to 'Export CSV', project_grade_download_path(@project, @grade), target: '_blank', class: 'btn btn-small btn-warning' %>
